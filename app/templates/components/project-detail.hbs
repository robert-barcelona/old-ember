  {{#if project}}


  <div class="panel panel-default project-box">
    <div class="panel-heading">
      <h3 class="panel-title">{{project.projectTitle}}</h3>
    </div>
    <div class="panel-body">
      <p>{{project.projectText}}</p>
      <p ><img class="projectImage" src={{project.projectImage}} /></p>
    </div>
  </div> 
  {{/if }}
